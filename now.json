{
    "version": 2,
    "name": "Q&A-cw-lite",
    "routes": [
      { "src": "/static/(.*)", "dest": "/dist/$1" },
      { "src": "/(.*)", "dest": "/backend/server.js" }
    ],
    "builds": [
      { "src": "dist/*", "use": "@vercel/static" },
      { "src": "backend/server.js", "use": "@vercel/node" }
    ]
  }
  
